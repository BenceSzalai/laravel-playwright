{
    "name": "didix16/laravel-playwright",
    "description": "Laravel Playwright Boilerplate",
    "keywords": [
        "laravel",
        "playwtight",
        "end-to-end"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yoann Frommelt",
            "email": "yoann@web-id.fr"
        },
        {
            "name": "Dídac Rodríguez",
            "email": "drodriguez816@gmail.com"
        }
    ],
    "homepage": "https://github.com/didix16/laravel-playwright",
    "autoload": {
        "psr-4": {
            "didix16\\LaravelPlaywright\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/routing": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "didix16\\LaravelPlaywright\\PlaywrightServiceProvider"
            ]
        }
    }
}
